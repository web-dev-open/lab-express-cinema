<a href="/movies">Go back</a>
<br />
<img src={{movie.image}} />
<h1>{{movie.title}}</h1>
<h2>{{movie.director}}</h2>
<br />
<p>Stars</p>
<ul>
  {{#each movie.stars}}
    <li>{{this}}</li>
  {{/each}}
</ul>
<br />
<p>{{movie.description}}</p>
<br />
{{#each movie.showtimes}}

  <span>{{this}} {{#unless @last}} | {{/unless}} </span>
{{/each}}