<div>
    

    <img src="{{movie.image}}" alt="">
    <div>
        
        <a href="/movies">Go Back</a>
        <h1>{{movie.title}}</h1>
        <h2>{{movie.director}}</h2>
    </div>
        <p>Stars:</p>
        <ul>
            {{#each movie.stars}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p>{{movie.description}}</p>
        <div class="showtime">
            {{#each movie.showtimes}}
            <span>{{this}} |</span>
            {{/each }}
        </div>
    </div>
    
</div>
