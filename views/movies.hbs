<h1>Ironhack Cinema</h1>

<div class='movies'>
    {{#each movie}}
        <div class="movieOne">
        <section>
            <a href="/movies/{{this._id}}"><img src="{{this.image}}" /></a>
            <h2>{{this.title}}</h2>
        <a href="/movies/{{this._id}}">See More</a>
        </section>
        </div>
    {{/each}}
</div>